{
  "name": "incontact",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=14.15.4",
    "npm": ">=6.14.10"
  },
  "scripts": {
    "test:unit": "lerna run test:unit",
    "lint": "lerna run lint",
    "lint:fix": "lerna run lint:fix",
    "clean": "lerna run clean && yarn run clean:node_modules",
    "clean:node_modules": "rimraf node_modules",
    "reset": "yarn run clean && yarn install",
    "start:ui": "cd apps/ui && yarn start",
    "start:scanner": "cd apps/data-scanner && yarn start",
    "start:analyzer": "cd apps/data-analyzer && yarn start",
    "start:monitor:sr": "cd apps/monitor-server && yarn start",
    "start:monitor:cl": "cd apps/monitor-client && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yzhbankov/inContact#readme"
  },
  "workspaces": [
    "shared/logger",
    "shared/repository",
    "shared/notificator",
    "shared/db-migration",
    "shared/env",
    "apps/data-analyzer",
    "apps/data-scanner",
    "apps/monitor-server",
    "apps/monitor-client",
    "apps/ui"
  ],
  "private": true,
  "dependencies": {
    "lerna": "^4.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^13.13.30",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-mocha": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "2.2.1"
  },
  "license": "UNLICENSED"
}
