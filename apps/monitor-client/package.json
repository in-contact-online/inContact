{
    "name": "incontact-monitor-client",
    "version": "1.0.0",
    "description": "",
    "main": "index.jsx",
    "engines": {
        "node": ">=14.15.4",
        "npm": ">=6.14.10"
    },
    "scripts": {
        "test:unit": "mocha --es-module-specifier-resolution=node lib/**/*.test.mjs",
        "test:unit:coverage": "cross-env NODE_ENV=test c8 yarn test:unit",
        "test:coverage": "cross-env NODE_ENV=test c8 mocha --exit --es-module-specifier-resolution=node --timeout 10000 e2e/integration/*.test.mjs lib/**/*.test.mjs",
        "test:ci": "yarn test:unit && yarn test:integration:ci",
        "lint": "eslint .",
        "lint:fix": "eslint --fix --ext .mjs .",
        "reset": "yarn clean && yarn",
        "clean": "rimraf node_modules",
        "prep-build": "",
        "preversion": "",
        "start": "NODE_ENV=development webpack-dev-server --host 0.0.0.0 --hot --port 8086",
        "build": "NODE_ENV=production webpack"
    },
    "dependencies": {
        "clean-webpack-plugin": "^4.0.0",
        "dotenv-webpack": "^7.1.0",
        "html-webpack-plugin": "^5.5.0",
        "mini-css-extract-plugin": "^2.6.0",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.11.1",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-syntax-class-properties": "^7.12.1",
        "@babel/polyfill": "^7.12.1",
        "@babel/preset-env": "^7.11.0",
        "@babel/preset-react": "^7.10.4",
        "c8": "^7.9.0",
        "mocha": "^8.4.0",
        "supertest": "^6.1.3",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1"
    },
    "c8": {
        "exclude": [
            "e2e/**",
            "app.mjs",
            "**/*.test.mjs"
        ],
        "check-coverage": true,
        "skip-full": false,
        "all": true
    },
    "license": "UNLICENSED"
}