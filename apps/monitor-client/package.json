        "ts-loader": "^9.3.0",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.8.1"
    },
    "c8": {
        "exclude": [
            "e2e/**",
            "app.mjs",
            "**/*.test.mjs"
        ],
        "check-coverage": true,
        "skip-full": false,
        "all": true
