        "@mui/x-data-grid": "^5.12.1",
        "@types/jest": "^27.5.0",
        "@types/node": "^17.0.31",
        "@types/react": "^18.0.9",
        "@types/react-dom": "^18.0.3",
        "axios": "^0.27.2",
        "file-loader": "^6.2.0",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
